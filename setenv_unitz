export PATH=.:$PATH

export CURR_DIR=`pwd`
export PATH=$CURR_DIR:$PATH
export PYTHONPATH=$CURR_DIR:$PYTHONPATH

if [ "$UNITZ_HOME" = "" ]; then
    export UNITZ_HOME=$CURR_DIR
fi

export PATH=$UNITZ_HOME:$PATH
export PYTHONPATH=$PYTHONPATH:$UNITZ_HOME/lib
export PYTHONPATH=$PYTHONPATH:$UNITZ_HOME/config
export PYTHONPATH=$PYTHONPATH:$UNITZ_HOME/std_units
export PYTHONPATH=$PYTHONPATH:$UNITZ_HOME/tests
export PYTHONPATH=$UNITZ_HOME:$PYTHONPATH

